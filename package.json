{
  "name": "ewhan-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "check-types": "tsc --noEmit --pretty --incremental false",
    "clean": "rimraf .next out",
    "db-reset": "prisma migrate dev reset",
    "db-seed": "prisma db seed",
    "dev": "pnpm dx:next",
    "dx": "run-p dx:* --print-label",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "generate": "prisma generate",
    "lint": "eslint --cache .",
    "lint-fix": "pnpm lint --fix",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate deploy",
    "migrate-dev": "prisma migrate dev",
    "postinstall": "pnpm generate",
    "prebuild": "run-s generate migrate",
    "prepare": "husky || true",
    "prisma-studio": "prisma studio"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "lucide-react": "^0.418.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@eslint-react/eslint-plugin": "^1.7.1",
    "@next/bundle-analyzer": "^14.2.5",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.14.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.40",
    "prisma": "^5.17.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
