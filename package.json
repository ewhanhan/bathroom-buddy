{
  "name": "bathroom-buddy",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "next build",
    "build-stats": "dotenv -e $npm_package_envFile -- cross-env ANALYZE=true pnpm run build",
    "check-types": "tsc --noEmit --pretty --incremental",
    "clean": "rimraf .next out",
    "db-reset": "dotenv -e $npm_package_envFile -- prisma migrate dev reset",
    "db-seed": "dotenv -e $npm_package_envFile -- prisma db seed",
    "dev": "dotenv -e $npm_package_envFile -- pnpm dx:next",
    "dx": "run-p dx:* --print-label",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "dotenv -e $npm_package_envFile -- pnpm prisma-studio",
    "generate": "prisma generate",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate deploy",
    "migrate-dev": "dotenv -e $npm_package_envFile -- prisma migrate dev",
    "postinstall": "pnpm generate",
    "prebuild": "run-s generate migrate",
    "prepare": "husky || true",
    "prisma-studio": "dotenv -e $npm_package_envFile -- prisma studio",
    "start": "next start",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^3.9.1",
    "@phosphor-icons/react": "^2.1.7",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "@vis.gl/react-google-maps": "^1.4.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "embla-carousel-react": "^8.5.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "next": "14.2.16",
    "next-auth": "5.0.0-beta.25",
    "next-cloudinary": "^6.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@eslint-react/eslint-plugin": "^1.23.0",
    "@next/bundle-analyzer": "^15.1.3",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.62.9",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.17.11",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.16",
    "eslint-plugin-format": "^0.1.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "prisma": "^6.1.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "envFile": ".env.local",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
