{
  "name": "bathroom-buddy",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "next build",
    "build-stats": "dotenv -e $npm_package_envFile -- cross-env ANALYZE=true pnpm run build",
    "check-types": "tsc --noEmit --pretty --incremental",
    "clean": "rimraf .next out",
    "db-reset": "dotenv -e $npm_package_envFile -- prisma migrate dev reset",
    "db-seed": "dotenv -e $npm_package_envFile -- prisma db seed",
    "dev": "dotenv -e $npm_package_envFile -- pnpm dx:next",
    "dx": "run-p dx:* --print-label",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "dotenv -e $npm_package_envFile -- pnpm prisma-studio",
    "generate": "prisma generate",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate deploy",
    "migrate-dev": "dotenv -e $npm_package_envFile -- prisma migrate dev",
    "postinstall": "pnpm generate",
    "prebuild": "run-s generate migrate",
    "prepare": "husky || true",
    "prisma-studio": "dotenv -e $npm_package_envFile -- prisma studio",
    "start": "next start",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@hookform/resolvers": "^3.9.0",
    "@phosphor-icons/react": "^2.1.7",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-query-devtools": "^5.51.23",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "@vis.gl/react-google-maps": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^2.4.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "embla-carousel-react": "^8.1.8",
    "framer-motion": "^11.3.28",
    "lucide-react": "^0.428.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "next-cloudinary": "^6.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@eslint-react/eslint-plugin": "^1.10.1",
    "@next/bundle-analyzer": "^14.2.5",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@types/node": "^20.15.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.41",
    "prisma": "^5.18.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "envFile": ".env.local",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
